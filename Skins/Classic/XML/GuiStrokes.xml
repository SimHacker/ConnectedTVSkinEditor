<gui
  title="Strokes"
  width="160"
  height="160"
  browser="Strokes"
  cachetype="-1"
>

  <!--  Copyright 2002-2003 ConnectedMedia Corporation   --> 

  <button
    label="Strokes &amp; Shortcuts"
    x="1" y="1"
    width="90" height="13"
