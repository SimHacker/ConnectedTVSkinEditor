  />
  <button
    label=""
    comment="Rating down arrow"
    x="131" y="110"
    width="7" height="7"
    framed="0" active="0"
  />

  <button
    x="0" y="135"
    width="160" height="24"
    labeldx="2" labeldy="6"
    pieup="  cancel:"
    piedown="  cancel:"
    columns="4"
  >

    <cell
      label="  Home"
      pietouch=":gotopage Home"
    />

    <cell
      label="   Help"
      pietouch=":gotopage Help"
    />

    <cell
      label="  More"
      pietouch=":gotopage MoreHelp"
    />

    <cell
      label="  Shows"
      pietouch=":gotopage ConnectedTV"
    />

