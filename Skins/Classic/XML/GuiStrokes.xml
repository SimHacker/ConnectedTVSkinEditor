    x="133" y="25"
    width="7" height="7"
    framed="0" active="0"
  />


  <button
    label="Guide Time &amp; Day"
    x="32" y="45"
    width="85" height="20"
    labeldx="3" labeldy="4"

    pietouch="Now:"
    pieup="Next 1/2 hour:"
    piedown="Previous 1/2 hour:"
    pieleft="Set Time:"
    pieright="Set Day:"

  />
  <button
    label="Set Time"
    x="4" y="40"
    labeldx="0" labeldy="4"
    width="25" height="30"
    framed="0" active="0"
    wrap="1"
